<?xml version="1.0"?>
<config
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="urn:magento:framework:Communication/etc/communication.xsd"
>
	<topic name="incomaker.event.product" request="string">
		<handler name="incomakerEventProductHandler" type="Incomaker\Magento2\Async\EventProduct\EventProductEventConsumer" method="process"/>
	</topic>
	<topic name="incomaker.event.order" request="string">
		<handler name="incomakerEventOrderHandler" type="Incomaker\Magento2\Async\EventOrder\EventOrderEventConsumer" method="process"/>
	</topic>
	<topic name="incomaker.event.user" request="string">
		<handler name="incomakerEventUserHandler" type="Incomaker\Magento2\Async\EventUser\EventUserEventConsumer" method="process"/>
	</topic>
	<topic name="incomaker.event.add-contact" request="string">
		<handler name="incomakerEventAddContactHandler" type="Incomaker\Magento2\Async\EventAddContact\EventAddContactEventConsumer"
				 method="process"/>
	</topic>
</config>
