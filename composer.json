{
	"name": "incomaker/magento2",
	"description": "Marketing automation with artificial intelligence",
	"type": "magento2-module",
	"version": "1.4.0",
	"authors": [
		{
			"name": "Incomaker",
			"email": "support@incomaker.com"
		}
	],
	"require": {
		"php": "^7.4|^8.2",
		"ext-simplexml": "*",
		"ext-json": "*",
		"incomaker/api": "^3.1.12",
		"magento/framework": "^103",
		"magento/module-quote": "^101",
		"magento/module-customer": "^103",
		"magento/module-webapi-async": "^100"
	},
	"license": "GPL-3.0-or-later",
	"autoload": {
		"files": [
			"registration.php"
		],
		"psr-4": {
			"Incomaker\\Magento2\\": ""
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://repo.magento.com/"
		}
	],
	"config": {
		"allow-plugins": {
			"magento/composer-dependency-version-audit-plugin": true
		}
	},
	"require-dev": {
		"phpstan/phpstan": "^1.10"
	}
}
